{
  "name": "stupid_password_manager",
  "description": "A very simple password manager",
  "author": "Wayne Lai; waynelai1228@github",
  "version": "1.0.0",
  "main": "src/main.js",
  "scripts": {
    "start": "node_modules/.bin/electron-forge start",
    "package": "node_modules/.bin/electron-forge package",
    "make": "node_modules/.bin/electron-forge make"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^9.0.5",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "jquery": "^3.5.1"
  },
  "dependencies": {
    "electron-compile": "^6.4.4",
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "stupid_password_manager"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "stupid_password_manager"
      }
    }
  }
}
